{
  "overrides": [
    {
      "files": "*.js",
      "excludedFiles": ["test/**"],
      "rules": {
        "builtin-compat/no-incompatible-builtins": "warn",
        "strict": ["error", "global"]
      },
      "settings": {
        "builtin-compat-ignore": [
          "Temporal\\..*",
          "SharedArrayBuffer\\.*",
          "TypedArray\\..*",
          "Generator\\..*",
          "AsyncGenerator\\..*",
          "WeakSet\\..*",
          "Atomics\\..*"
        ]
      }
    }
  ]
}
