{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": "google",
    "plugins": ["eslint-plugin-builtin-compat"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "require-jsdoc": "off",
        "indent": ["error", 2],
        "max-len": "off"
    },
    "ignorePatterns": ["sorttable.js"]
}
